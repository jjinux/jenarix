
# Jenarix functions statically close on free variables using the
# surrounding scope

(set x [1,2,3]);

(set z [3,4,5]);

(def myfn [[x,y],{y:[2,3,4]}] ((x,y,z)));

(impl myfn);

(myfn);

# result: [ [1, 2, 3], [2, 3, 4],
#           [3, 4, 5]]

(myfn 1);

# result: [1, [2, 3, 4],
#           [3, 4, 5]]

(myfn 1 2);

# result: [1,2,
#           [3, 4, 5]]

